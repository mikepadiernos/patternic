// SOCIAL LINKS

ul
	padding 0
	list-style none
	&.menu-social
		&.social-icons
			display flex
			justify-content center
			align-items center
			a
				text-decoration none
				transition 0.6s
				display: flex;
				justify-content: center;
				align-items: center;
				&.fa
					font-size 1.35rem
					padding 0.5rem
					color body
					background-color primary
					border-radius 50%
					margin 0 4px
					width 32px
					height 32px
				&:hover
					&.fa
						background-color text