// SEEDS TOOLBAR

#toolbar-administration
  &.seeds_toolbar
    #toolbar-bar
      background #000000
      > .toolbar-tab
        > a,
        > button
          width 55px
          height 50px
          margin-right 0
          &:hover
            background #4d4d4d
        #toolbar-item-primary-tasks-tray
          a,
          button
            padding-right 20px !important
            padding-left 20px !important
        .account
          width 100%
